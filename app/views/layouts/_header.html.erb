<div class = "wrapper">
	<header id="mainheader">
		<div id="logo"><%= link_to image_tag("sourceuno_logo.png", alt: "Source Uno"), 'http://sourceuno.com' %>
		</div>
		<div class='login'>
		  <% if current_user %>
		    Logged in as <%= current_user.name %>.
		    <%= link_to "View Profile", current_user %> / 
		    <%= link_to "Edit Profile", edit_user_path(current_user) %> or
		    <%= link_to "Sign Out", signout_path, method: "delete" %>
		
		  <% else %>
            <%= link_to "Sign In", signin_path %> or
		    <%= link_to "Start Now", signup_path%>
         
		  <% end %>
	  </div>
		<nav id="mainnav">
			<ul>
			  <li><%= link_to "Home",    root_path %></li>
			  <li><%= link_to "Professions", professions_path %></li>
			  <li><%= link_to "Employers", employers_path %></li>
			  <li><%= link_to "Help",    help_path %></li>
			</ul>
		</nav>
	</header>
	  	   